    "express": "4.18.2",
    "handlebars": "4.7.7",
    "nanoid": "4.0.0"
  },
  "engines": {
    "npm": "8.11.0",
    "node": "16.15.1"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": ">= 50%"
      }
    }
  },
  "alias": {
    "src": "./src",
    "static": "./static",
    "test": "./test"
